{
    "name": "@wartab/vat-number-validator",
    "repository": {
        "type": "git",
        "url": "https://github.com/wartab/vat-number-validator"
    },
    "version": "1.0.2",
    "description": "",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "file": "./dist/index.mjs",
                "types": "./dist/index.d.ts"
            },
            "require": {
                "file": "./dist/index.cjs",
                "types": "./dist/index.d.ts"
            }
        }
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=16"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/index.mjs && esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.cjs && tsc --emitDeclarationOnly --declaration --outDir dist",
        "test": "vitest run"
    },
    "keywords": [
        "vat",
        "vat number",
        "typescript",
        "vat validator"
    ],
    "author": "wartab",
    "license": "MIT",
    "devDependencies": {
        "esbuild": "0.25.2",
        "typescript": "5.8.3"
    }
}
